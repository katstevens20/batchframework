#!/usr/bin/env php
<?php

use Kat\BatchFramework\Core\Console;

require __DIR__ . '/../vendor/autoload.php';

date_default_timezone_set('Europe/Paris');

try {
    $console = new Console();
    $console($argv, __DIR__ . '/../.env');
} catch (Exception $e) {
    echo "\nError: " . $e->getMessage() . "\n";
    echo $e->getTraceAsString();
    exit(1);
}


