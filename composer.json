{
  "name": "batch-framework",
  "description": "Core framework for jobs in php 7.4",
  "authors": [
    {
      "name": "Khalid ATLASSI",
      "email": "khalid.atlassi@gmail.com"
    }
  ],
  "require": {
    "monolog/monolog": "^1.26",
    "ext-pdo": "*",
    "symfony/dotenv": "*"
  },
  "autoload": {
    "psr-4": {
      "Kat\\BatchFramework\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^9.5.19",
    "pestphp/pest": "^1.21",
    "phpseclib/phpseclib": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": true,
    "platform": {
      "php": "7.4"
    }
  }
}